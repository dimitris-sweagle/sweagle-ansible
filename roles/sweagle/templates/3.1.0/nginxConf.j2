under http {}

		## SWEAGLE settings
        client_max_body_size 		200M;
        proxy_connect_timeout       300000;
        proxy_send_timeout          300000;
        proxy_read_timeout          300000;
        send_timeout                300000;
        fastcgi_read_timeout    	1800000;

        gzip_vary on;
        gzip_proxied any;
        gzip_comp_level 5;
        gzip_min_length 256;
        gzip_types text/plain text/css application/json application/javascript text/xml application/xml application/xml+rss text/javascript font/opentype application/vnd.siren+json;